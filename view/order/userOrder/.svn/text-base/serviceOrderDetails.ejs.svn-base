<style type="text/css">
    body{ padding-bottom: .9rem!important;}
    .more:after{ right: .2rem;}
    /*交易状态*/
    .content>div.hd{position: relative; background-color: #dd2726; font-size: .26rem; height: 1.8rem; line-height: 1.8rem; padding: 0 .5rem; box-sizing: border-box; color: #fff;}
    .content>div.hd>i{ font-size: 1.5rem; float: right; color:#f0f0f0; opacity: 0.3;}
    .content>div.Info{ padding: .2rem; box-sizing: border-box; min-height:1rem; position: relative;}
    .content>div.Info>i{ position: absolute; left: .2rem; top: .2rem; line-height: .6rem; font-size: .42rem; color: #999;}
    .content>div.Info>p{ line-height: .3rem; padding-left: .6rem; box-sizing: border-box; font-size: .24rem; color: #666;}
    .content>div.hd>div{width: 70%;  line-height: .5rem; position: relative;}
    .content>div.hd>div>h5{ color: #fff; padding-top: .4rem;}
    .content>div.hd>i.meiyuan{ position: absolute; right: .1rem; top: 0;}
    .content>div.Logistics{ padding: .2rem; box-sizing: border-box; min-height:1rem; position: relative; border-bottom: 1px solid #e5e5e5;}
    .content>div.Logistics>i{ position: absolute; left: .2rem; top: .2rem; line-height: .6rem; font-size: .42rem; color: #999;}
    .content>div.Logistics>p{ line-height: .3rem; padding-left: .6rem; box-sizing: border-box; font-size: .24rem;}

    /*商品*/
    .con-list{ margin-bottom: .1rem; margin-top: .1rem;}
    .con-list .hd{ height: .8rem; line-height: .4rem; padding: .2rem; box-sizing: border-box;}
    .con-list .hd>a{ font-size: .26rem;}
    .con-list .hd>a>span{ width: .4rem; height: .4rem; display: inline-block;}
    .con-list .hd>a>span>img{ width: 100%;}
    .con-list>ul>li{ padding: .2rem; min-height: 1.45rem; border-bottom: 1px solid #e5e5e5;}
    .con-list>ul>li:last-child{ border-bottom: 0;}
    .con-list>ul>li>a{ display: block; position: relative; padding:0 1.1rem 0 1.7rem; box-sizing: border-box; }
    .con-list>ul>li>a>div.pic{ width: 1.5rem; border-radius: 3px; overflow: hidden; position: absolute;left: 0; top: 0;}
    .con-list>ul>li>a>div.pic>img{width: 100%;}
    .con-list>ul>li>a>div.jiage{ position: absolute; right: 0; top: 0; text-align: right;}
    .con-list>ul>li>a>div.jiage>button{ margin-top: .05rem; padding:2px .3rem; border: 1px solid #e5e5e5; border-radius: 3px; color: #666;background-color: #fff;}
   /*商品总价*/
   	.con-list .totalPrice>p{ padding:.1rem .2rem; box-sizing:border-box;}
    .con-list .totalPrice>li{box-sizing: border-box; padding:0 .2rem; border-bottom:1px solid #e5e5e5;min-height:.8rem; height:.8rem;line-height:.8rem;}
    /*实付款*/
    .con-list .Payment{ height: .8rem; line-height: .8rem; padding: 0 .2rem; box-sizing: border-box; background-color: #fcfafb; position: relative;}
    .Payment>i{ width:.1rem; height:.1rem; content:''; border-top:1px solid #e5e5e5; border-right:1px solid #e5e5e5;background-color: #fcfafb; display:block; margin-left:auto; transform:rotate(-45deg); position: absolute;right: .4rem; top: -.06rem;}
    /*返积分*/
    .integral>ul{background-color: #fff;}
    .integral>ul>li{ border-bottom: 1px solid #e5e5e5; height:.8rem; line-height: .4rem; padding: .2rem; box-sizing: border-box;}
    .integral>ul>li>a{ display: block;}
    .integral>ul>li:last-child>a{display: inline-block; width: 35%;}
    .integral>ul>li:last-child>a:last-child{ border-left: 1px solid #e5e5e5;}
    .integral>ul>li:last-child>a:last-child>i{ font-size: .5rem;}
    .integral>ul>li>a>i{ font-size: .4rem; margin-right: .2rem;vertical-align: middle;}
    .orderinfo{ position: relative;}
    .orderinfo>div{ padding: .2rem; box-sizing: border-box;}
    .orderinfo>div>p{ color: #999; line-height: .35rem;}
    .orderinfo>button{position: absolute; right: .2rem; top: .2rem; border:1px solid #e5e5e5; border-radius: 3px; padding: 2px .3rem; color: #666; background-color: transparent;}
    .one1,.one2{ display: block;}
    .footer{ width:100%; min-width:320px; max-width:640px; position:fixed; bottom:0; height:.8rem; padding:.15rem 0; box-sizing:border-box; background-color:#fff; border-top:1px solid #ddd;}
    .footer>button{float:right; margin-right:.5rem; color:#666; width:1.5rem; border:1px solid #999; font-size:.26rem; height:.5rem; line-height:.5rem; background:#fff; text-align:center;}
    /*退款进度*/
	.schedule{line-height:.7rem; height:.7rem; border-top:1px solid #e5e5e5; text-indent:.2rem;  position:relative; }
	.schedule>a{display:block;}
</style>
<header class="navbar navbar2">
    <div class="nav-wrap-left">
        <a href="javascript:void(0);" class="color-gray3" id="returnBtn">
            <i class="iconfont">&#xe60b;</i>
            <s></s>
        </a>
    </div>
    <span class="nav-header h1 color-gray3">订单详情</span>
    <div class="nav-wrap-right">
        <a href="javascript:;" class=" color-gray3">
            <span class="nav-btn">
                <i class="iconfont icon-info">&#xe636; </i>
            </span>
            <s></s>
        </a>
    </div>
    <div class="nav-dropdown">
        <span class="triangle-up"></span>
        <ul>
            <li>
                <a class="react" href="/">
                    <i class="iconfont">&#xe602;</i>
                    <space></space>首页
                </a>
            </li>
            <li>
                <a class="react" href="javascript:;">
                    <i class="iconfont">&#xe603;</i>
                    <space></space>搜索
                </a>
            </li>
            <li>
                <a class="react" href="javascript:;">
                    <i class="iconfont">&#xe601;</i>
                    <space></space>消息
                    <em class="information">8+<s class="triangle-up"></s></em>
                </a>
            </li>
        </ul>
    </div>
</header>
<div class="content">
    <div class="Info bg-white one2" id="receiver">
        <i class="iconfont">&#xe616;</i>
        <p>买家昵称：<%=_datas.data.userNickName%></p>
        <p>手机号：<%=_datas.data.mobilePhone%></p>
    </div>
    <%if(_datas.data.status=='payCan' && _datas.data.refundsId != null){%>
	    <div class="schedule more bg-white"  onclick="this.ctrl.refundsDetails(<%=_datas.data.refundsId%>)">
	    	<a href="javascript:void(0);">
	    		退款进度
	    	</a>
	    </div>
	<%}%>
    <div class="con-list">
        <div class="hd bg-white" onclick="this.ctrl.toShop(<%=_datas.data.shopId%>);">
            <a href="javascript:;">
                <span>
                    <img src="<%=Config.imgPre+_datas.data.avatar%>"/>
                </span>
                <%=_datas.data.shopName%> >
            </a>
        </div>
        <ul>
            <li onclick="this.ctrl.toProductDetails(<%=_datas.data.orderItems[0].goodsId%>,<%=_datas.data.orderItems[0].shopId%>,<%if(_datas.data.orderItems[0].specialtyContentId==null){%>null<%}else{%><%=_datas.data.orderItems[0].specialtyContentId%><%}%>)">
                <a href="javavscript:void(0);">
                    <div class="pic">
                        <img src="<%=Config.imgPre+_datas.data.orderItems[0].thumbnail%>"/>
                    </div>
                    <div class="txt">
                        <h6 class="color-gray6  ellips1""><%=_datas.data.orderItems[0].name%></h6>
                        <p class="color-gray9"><%if(_datas.data.orderItems[0].skuValue==null){%><%}else{%><%=_datas.data.orderItems[0].skuValue%><%}%></p>
                    </div>
                    <div class="jiage">
                        <span class="color-gray3">￥<%=_datas.data.orderItems[0].price.toFixed(2)%></span>
                        <p class="color-gray6">x<%=_datas.data.orderItems.length%></p>
                    </div>
                </a>
            </li>
        </ul>
    	<%if(_datas.data.status=="noCon" || _datas.data.status=="hasCon" || _datas.data.status=="hasCom"){%>
        	<div class="totalPrice bg-white">
    			<%$.each(_datas.data.orderItems,function(index,orderItem){%>
	        		<p class="color-gray6">
	        			消费码<%=index+1%>:&nbsp;
	        			<%=orderItem.consumptionKey.substr(0,3)%>&nbsp;<%=orderItem.consumptionKey.substr(3,3)%>&nbsp;<%=orderItem.consumptionKey.substr(6,3)%>
	        			<%if(orderItem.isConsumption){%>
	        				
	        			<%}else{%>
		        			<span onclick="this.ctrl.genarateCode('<%=orderItem.id%>')" style="color:rgba(0, 0, 255, 0.71);">显示二维码</span>
	        			<%}%>
	        			<cite class="kdl-right">
	        				<%if(orderItem.isConsumption){%>
	        					已消费 
	        				<%}else{%>
	        					未消费 
	        				<%}%>
	        			</cite>
	        		</p>
	    		<%})%>
	    		<%if(_datas.data.status=="noCon" && _datas.data.expiryTime!=null){%>
	    			<p class="color-gray6">消费码有效期至：<%=_datas.data.expiryTime%></p>
		    	<%}%>
    		</div>
    	<%}%>
        <ul class="totalPrice bg-white">
            <li class="color-gray3 bg-silver">支付方式：&nbsp;<cite>在线支付</cite></p>
            <li class="color-gray3">商品金额：&nbsp;<cite class="color-red">￥<%=_datas.data.goodsPrice.toFixed(2)%></cite></li>
            <li class="color-gray3">返现：&nbsp;<cite class="color-red">￥<%=_datas.data.returnAmount.toFixed(2)%></cite></li>
            <li class="color-gray3"><%if(_datas.data.status=="pePay"){%>应支付金额：&nbsp;<cite class="color-red">￥<%=(_datas.data.payAmount).toFixed(2)%></cite><%}else{%>实付款：&nbsp;<cite class="color-red">￥<%=(_datas.data.payAmount+_datas.data.useBalance).toFixed(2)%></cite><%}%></li>
        </ul>
    </div>
    <!--返积分-->
    <div class="integral m-top10">
        <ul>
            <li onclick="this.ctrl.toCallBus();">
                <a href="javascript:;">
                    <i class="iconfont color-orange">&#xe601;</i>联系卖家
                </a>
                <!--
                <a href="javascript:;">
                    <i class="iconfont color-orange">&#xe634;</i>拨打电话
                </a>
                -->
            </li>
        </ul>
    </div>
    <div class="orderinfo m-top10 bg-white">
        <div>
            <p>订单编号：<%=_datas.data.sn%></p>
            <p>创建时间：<%=_datas.data.createDate%></p>
        </div>
    </div>
</div>
<%if(_datas.data.status=='sysAud' || _datas.data.status=='noCon'){%>
	<div class="footer"  id="payCancelBtn">
    	<button type="button" onclick="this.ctrl.payCancel(<%=_datas.data.orderId%>)">退款</button>
	<div>
<%}%>
