<style type="text/css">
    body, html { height: 100%;}
    #container { position: relative;height: 100%; width: 100%; min-height: 4rem; margin-top: 2rem;}
    #container button {padding: 20px;border: none;background: transparent;display: block; position: relative;z-index: 3;margin: 0 auto;}
    .dot {height: 2px;width: 2px; border-radius: 50%; -moz-border-radius: 50%; -webkit-border-radius: 50%;position: absolute;z-index: 0;animation: sploosh 50s cubic-bezier(0.165, 0.84, 0.44, 1);background: transparent; }
    @keyframes sploosh {
        0% {
            box-shadow: 0 0 0 .5rem rgba(66, 166, 223, 0.7);
            background: rgba(66, 166, 223, 0.7);
            -moz-border-radius:  0 0 0 .5rem rgba(66, 166, 223, 0.7); /* Firefox */
            -webkit-border-radius: 0 0 0 .5rem rgba(66, 166, 223, 0.7);/* Safari 和 Chrome */
        }
        100% {
            box-shadow: 0 0 0 4rem rgba(66, 166, 223, 0);
            background: rgba(66, 166, 223, 0);
            -moz-border-radius:  0 0 0 4rem rgba(66, 166, 223, 0);/* Firefox */
            -webkit-border-radius: 0 0 0 4rem rgba(66, 166, 223, 0);/* Safari 和 Chrome */
        }
    }
    body{background-color: #3a3e49; }
    .navbar{ background-color: #323642;}
    .content .ptos{ width: 1.2rem; position: absolute; text-align: center;top: 4.3rem;left: 50%;margin-left: -.6rem; z-index: 99;}
    .content .ptos>img{ width: 100%;}
    .con-swish{height: 100%;}
    .con-swish>h3{ position: absolute;text-align: center;top: 6rem;left: 50%;margin-left: -2rem; z-index: 99; width: 4rem;font-size: .26rem !important;}
</style>
