<style type="text/css">
	.pic>img{width:100%;}
    .navbar .nav-header{ color: #333;}
    .content{position: relative; padding-bottom: 1rem;}
    .content .hd-tit{background-color: #fff; padding: .3rem; box-sizing: border-box;}
    .content .hd-tit>ul>li{ float: left; width: 50%;}
    .content .hd-tit>ul>li>a{ display: block; color: #666; text-align: center;}
    .content .hd-tit>ul>li>a>i{ width: 1rem; height: 1rem; line-height: 1rem; display: block; border-radius: 50%; color: #fff; text-align: center; font-size: .5rem;margin: 0 auto;}
    .content .hd-tit>ul>li>a>p{ font-size: .26rem; margin-top: .1rem; text-align: center;}
    /*筛选*/
    .content .hd{height:.8rem; line-height:.5rem; padding:.15rem 0; box-sizing: border-box;z-index: 2;position: relative; border-bottom: 1px solid #e5e5e5;}
    .content .hd>ul{width:100%; float:left;}
    .content .hd>ul>li{width:33.33%;float:left;border-right:1px solid #e5e5e5; box-sizing: border-box;}
    .content .hd>ul>li:last-child{border-right:0px;}
    .content .hd>ul>li.cur>a{ color:#d1322f;}
    .content .hd>ul>li.cur>a>i{ display: block;}
    .content .hd>ul>li>a{display:block; text-align:center;}
    .content .hd>ul>li>a>i{ display: none;}
    .content .hd>ul>li>a>em{ font-size: .15rem;vertical-align: middle; margin-left: 3px;}
    /*宝贝*/
    .content>ul{min-height: 5rem;}
    .content>ul>li{ border-bottom: 1px solid #e5e5e5;background-color: #fff;}
    .content>ul>li>a{ display: block; position: relative; padding: .1rem; padding-left: 2.5rem; box-sizing: border-box;}
    .content>ul>li>a>div.pic{ position: absolute; left: .1rem; top: .1rem; width: 2.3rem; border-radius: 5px; overflow: hidden; height: 2.2rem;}
    .content>ul>li>a>div.pic>img{ width: 100%; height:100%;}
    .content>ul>li>a>div.pic>i{ position: absolute; left: .1rem; top: .1rem; color: #999; font-size: .42rem; display: none;}
    .content>ul.batch>li>a>div.pic>i{display: block;}
    .content>ul>li>a>div.pic>div.bt{ position: absolute; bottom: 0; width: 100%; height: .6rem; line-height: .6rem;}
    .content>ul>li>a>div.pic>div.bt>div{ width: 100%; display: block; background-color: #000; opacity: 0.5; height: .6rem;}
    .content>ul>li>a>div.pic>div.bt>p{ display: block; color: #fff;height: .6rem; text-align: center; position: absolute;top: 0;z-index: 2; width: 100%;}
    .content>ul>li>a .txt{ padding-bottom: .2rem; border-bottom: 1px solid #e5e5e5;}
    .content>ul>li>a .txt>h6{height:.8rem;}
    .content>ul>li>a .txt>div{ padding-top: .1rem; position: relative;}
    .content>ul>li>a .txt>div>button{ border: 1px solid #e5e5e5; font-size: .24rem; color: #999; border-radius: 3px; background-color: transparent; padding:.06rem .2rem; }
    .content>ul>li>a .txt>div>i{ position: absolute; top: .1rem; right: 0; z-index: 2; font-size:.43rem;}
    .content>ul>li .box>ul{ padding: 0 .1rem;}
    .content>ul>li .box>ul>li{ float: left; width: 33.33%; height: auto; line-height: .5rem; font-size: .24rem; color: #666;}
    .theme-popover-mask{top: 4.5rem;}
    /*筛选条件*/
    .screenbox{position: absolute;z-index: 3; width: 100%; min-width: 320px;max-width: 640px; top: 3.84rem;}
    .screenbox ul>li{ line-height: .8rem; padding: 0 .1rem; border-bottom: 1px solid #e5e5e5;}
    .screenbox ul>li.cur a{ color: #df494b;}
    .screenbox ul>li.cur a>i{ display: block;}
    .screenbox ul>li>a{ display: block;}
    .screenbox ul>li>a>i{ display: none;}
    .content .box3{ padding: .2rem; background-color: #fff;}
    .content .box3>ul>li{ float: left; border-bottom: 0;}
    .content .box3>ul>li>a{ display: inline-block; height: .5rem; line-height: .5rem; width: 1.3rem; text-align: center; border: 1px solid #f0f0f0; border-radius: 3px; background-color: #f0f0f0; margin-right: .2rem;}
    .content .box3>ul>li.cur>a{ border: 1px solid #cc3333; color: #cc3333; background-color: #fff;}
    /*提示*/
    .reminderDiv{ padding: .1rem .2rem; box-sizing: border-box; position: relative; height: 1rem;}
    .reminderDiv>hr{ margin-top: .4rem;}
    .reminderDiv>span{ position: absolute; left: 50%; top:.3rem; width: 2.5rem; margin-left: -1.25rem; text-align: center; height: .4rem; line-height: .4rem; background-color: #f0f0f0;}
    /*footer*/
    .footer{display: none;  z-index: 9; padding-left: 1.5rem; box-sizing: border-box; position: fixed; bottom: 0; width: 100%; min-width: 320px;max-width: 640px;   height:.98rem; line-height: .98rem; background-color: #fff;}
    .footer>span{ position: absolute; left: .2rem;  top:0;}
    .footer>span>i{ font-size: .42rem;margin-right: 3px;vertical-align: middle;}
    .footer>ul>li{ float: left; width: 33.33%; padding: .2rem .1rem; box-sizing: border-box;}
    .footer>ul>li>a{ display: block;  height: .6rem; line-height: .6rem;  color: #fff; background-color: #999; text-align: center; border-radius: 5px;}
    /*添加宝贝*/
    .opo{ display: none; box-sizing: border-box; padding:.4rem .2rem; position:fixed; text-align:center; z-index:12; width:5rem; height:4.2rem; background:#fff; border-radius:.1rem;left:50%; margin-left:-2.5rem; top:50%; margin-top:-2.1rem;}
    .opo .hd{text-align: left; position:relative; padding-left:1.2rem; margin-bottom:.5rem;}
    .opo .hd p{ color: #999;}
    .opo .hd .pic{ position:absolute; left:0; top:0; width:1rem; height:1rem; border-radius:50%;}
    .opo .hd .pic img{width:100%; height:100%;}
    .opo .btn{text-align:center; font-size: .28rem;}
    .opo>span{position: absolute;display: inline-block;top: .15rem;right: .15rem;font-size: .42rem; color: #333;}
</style>
<header class="navbar navbar2">
