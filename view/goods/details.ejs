            <div class="pic">
                <img src="<%=Config.imgPre+_datas.shop.avatar%>"/>
            </div>
            <div class="txt">
                <h6><%=_datas.shop.shopName%><a href="tel:<%=_datas.shop.shopPhone%>"><i class="iconfont color-orange">&#xe61b;</i></a></h6>
                <div class="pto">
                    <img src="<%=Config.imgPre + _datas.shop.shopRank.icon %>" />
                </div>
                <p class="color-gray6 m-top5"><%=_datas.shop.shopAddress%><i id="entryMap" shopId="<%=_datas.goods.shopId%>" class="iconfont color-orange">&#xe616;</i></p>
            </div>
        </div>
        <%if(_datas.goods.goodsTypeId == "4"){%>
	        <ul class="hd-nav m-top10">
		    	<li>
		    		<a href="javascript:;">
		    			商品描述<cite class="color-red"><%=_datas.shop.description.toFixed(1)%></cite>
		    		</a>
		    	</li>
		    	<li>
		    		<a href="javascript:;">
		    			服务态度<cite class="color-red"><%=_datas.shop.service.toFixed(1)%></cite>
		    		</a>
		    	</li>
		    </ul>
        <%}else{%>
	        <div class="Infomation-bd m-top30 clearfix">
		        <ul>
		            <li>
		                <span class="color-gray3 allGoodsCount">0</span>
		                <p class="color-gray9">全部商品</p>
		            </li>
		            <%if(_datas.isAddCowrie){%>
		                <li>
		                    <span class="color-gray3 newGoodsCount">${_datas.sellCount}</span>
		                    <p class="color-gray9">人在卖</p>
		                </li> 
		            <%}else{%>
		                <li>
		                    <span class="color-gray3 newGoodsCount">0</span>
		                    <p class="color-gray9">新品</p>
		                </li> 
		            <%}%>
		            <li>
		                <span class="color-gray3 attentionPeoples">0</span>
		                <p class="color-gray9">收藏人数</p>
		            </li>
		        </ul>
